<Window x:Class="TraductorLenguajes.Registros.RestPassword.ElegirCuenta"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TraductorLenguajes.Registros.RestPassword"
        mc:Ignorable="d"
        Title="ElegirCuenta"
        Height="250" Width="350"
        MaxHeight="250" MaxWidth="350"
        MinHeight="250" MinWidth="350"
        WindowStartupLocation="CenterScreen">
    <Grid Background="#000">
        <Grid.RowDefinitions>
            <RowDefinition Height="0.45*"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>

        <Grid Grid.Row="0">
            <TextBlock Foreground="#ddd" FontSize="15" TextAlignment="Center">
                Cuantas vinculadas al gmail(haga doble click):
                <LineBreak/>
                <Label x:Name="info_gmail" Foreground="#ddd"/>
            </TextBlock>
        </Grid>
        <Grid Grid.Row="1">
            <ListBox x:Name="ListCuentas" Background="#111">

            </ListBox>
        </Grid>
    </Grid>
</Window>
